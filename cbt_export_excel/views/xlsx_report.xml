<?xml version="1.0" encoding="utf-8" ?>
<odoo>
    <record id="wizard_excel_report_wizard" model="ir.ui.view">
        <field name="name">wizard.excel.report</field>
        <field name="model">wizard.excel.report</field>
        <field name="arch" type="xml">
            <form string="Import Customers">
                <group>
                    <group>
                        <field name="excel_file"/>
                        <field name="file_name"/>
                    </group>
                </group>
                <footer>
<!--                    <button-->
<!--                            name="import_journal_entry"-->
<!--                            string="Import"-->
<!--                            type="object"-->
<!--                            class="btn-primary"-->
<!--                    />-->
                </footer>
            </form>
        </field>
    </record>


    <record id="import_customer_action" model="ir.actions.act_window">
        <field name="name">Export Customer</field>
        <field name="res_model">wizard.excel.report</field>
        <field name="view_mode">form</field>
        <field name="view_id" ref="wizard_excel_report_wizard"/>
        <field name="target">new</field>
    </record>

    <menuitem id="customer_import_menu"
              name="Export Customers"
              action="import_customer_action"
              parent="contacts.menu_contacts"/>
</odoo>